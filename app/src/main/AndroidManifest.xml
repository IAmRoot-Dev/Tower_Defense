<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iamroot.towerdefense.towerdefense">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayMenu"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SettingsMenu"
            android:screenOrientation="landscape" />
        <activity
            android:name=".StoreMenu"
            android:screenOrientation="landscape" />
        <activity
            android:name=".GuideMain"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AboutMain"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TermsMain"
            android:screenOrientation="landscape" />
        <activity
            android:name=".GameTest"
            android:screenOrientation="landscape" />
    </application>
</manifest>